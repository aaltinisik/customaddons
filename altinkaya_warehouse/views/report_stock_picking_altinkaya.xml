<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<template id="report_stock_picking_altinkaya_doc">
			<!-- <t t-name="report_stock_picking_altinkaya"> -->
			<t t-call="report.html_container">
				<div class="header">
					<table class="table-condensed"
						style="border-color: #000000;margin-right: 10px;margin-left: 10px;margin-bottom: 0px;margin-top: 10px;width: 100%;"
						border="2">
						<tbody>
							<tr>
								<td
									style="border-color: #ffffff; text-align: center; vertical-align: center;">
									<h2 style="margin-top: 2px;margin-bottom: 2px;">
										<span t-field="o.name" />
									</h2>
									<span style="margin-top: 2px;margin-bottom: 2px;" t-field="o.picking_type_id" />
								</td>
								<td
									style="border-color: #ffffff; text-align: center; vertical-align: center;">
									<svg xml:space="preserve" width="12mm" height="9mm"
										version="1.1"
										style="shape-rendering:geometricPrecision; text-rendering:geometricPrecision; image-rendering:optimizeQuality; fill-rule:evenodd; clip-rule:evenodd"
										viewBox="0 0 1200 900"><defs>
  <style type="text/css"> .fil0 {fill:#E5097F}</style></defs><g
										id="Katman_x0020_1"><metadata id="CorelCorpID_0Corel-Layer" /><path
										class="fil0"
										d="M688 693c37,18 67,49 86,86 21,-5 107,-30 161,-86 62,-65 71,-131 20,-181 -50,-51 -116,-42 -181,20 -56,54 -80,139 -86,161zm-148 -12l0 0c-11,-19 -55,-96 -53,-174 2,-90 42,-143 114,-143 71,0 112,53 113,143 2,78 -42,156 -53,174 -19,-6 -40,-10 -61,-10 -21,0 -41,4 -60,10zm-114 97l0 0c19,-37 50,-67 87,-86 -6,-21 -30,-106 -86,-161 -65,-62 -131,-71 -181,-20 -51,50 -42,117 20,181 54,56 139,80 160,86zm-344 -772l0 0 1036 0c41,0 74,33 74,74l0 814 -93 0c2,-8 3,-18 3,-27 0,-72 -53,-112 -143,-114 -78,-2 -155,42 -174,53 6,19 10,39 10,60 0,10 -1,19 -2,28l-386 0c-1,-9 -2,-18 -2,-28 0,-21 4,-42 10,-61 -18,-11 -96,-55 -174,-53 -90,1 -143,42 -143,113 0,11 1,20 3,29l-93 0 0 -814c0,-41 33,-74 74,-74z" /></g>
                                    </svg>
								</td>
								<td
									style="border-color: #ffffff; text-align: center; vertical-align: center;">
									<img
										t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', o.name, 600, 100)"
										style="width:300px;height:50px;" />
								</td>
							</tr>
						</tbody>
					</table>
				</div>
				<div class="page">
					<div>
						<div style="font-size:10px;">
							<br />
							<div t-if="o.picking_type_id.code=='incoming' and o.partner_id">
								<span>
									<strong>Tedarikçi Adresi:</strong>
									<t t-esc="o.partner_id.display_name" />
									<t t-esc="o.partner_id.street" />
									<t t-esc="o.partner_id.street2" />
									<t t-esc="o.partner_id.city" />
									<t t-esc="o.partner_id.state_id.name" />
									<t t-esc="o.partner_id.country_id.name" />
								</span>
							</div>
							<div t-if="o.picking_type_id.code=='internal' and o.partner_id">
								<span>
									<strong>Depo Adresi:</strong>
									<t t-esc="o.partner_id.display_name" />
									<t t-esc="o.partner_id.street" />
									<t t-esc="o.partner_id.street2" />
									<t t-esc="o.partner_id.city" />
									<t t-esc="o.partner_id.state_id.name" />
									<t t-esc="o.partner_id.country_id.name" />
								</span>
							</div>
							<div t-if="o.picking_type_id.code=='outgoing' and o.partner_id">
								<span>
									<strong>Müşteri Adresi:</strong>
									<t t-esc="o.partner_id.display_name" />
									<t t-esc="o.partner_id.street" />
									<t t-esc="o.partner_id.street2" />
									<t t-esc="o.partner_id.city" />
									<t t-esc="o.partner_id.state_id.name" />
									<t t-esc="o.partner_id.country_id.name" />
								</span>

							</div>
							<div
								t-if="o.move_lines and o.move_lines[0].partner_id and o.move_lines[0].partner_id.id != o.partner_id.id">
								<span>
									<strong>Teslimat Adresi:</strong>
									<t t-esc="o.move_lines[0].partner_id.display_name" />
									<t t-esc="o.move_lines[0].partner_id.street" />
									<t t-esc="o.move_lines[0].partner_id.street2" />
									<t t-esc="o.move_lines[0].partner_id.city" />
									<t t-esc="o.move_lines[0].partner_id.state_id.name" />
									<t t-esc="o.move_lines[0].partner_id.name" />
								</span>
							</div>
							<div
								t-if="o.picking_type_id.code != 'internal' and (not o.move_lines or not o.move_lines[0].partner_id) and o.picking_type_id.warehouse_id.partner_id">
								<span>
									<strong>Depo Adresi:</strong>
									<t t-esc="o.picking_type_id.warehouse_id.partner_id.display_name" />
									<t t-esc="o.picking_type_id.warehouse_id.partner_id.street" />
									<t t-esc="o.picking_type_id.warehouse_id.partner_id.street2" />
									<t t-esc="o.picking_type_id.warehouse_id.partner_id.city" />
									<t t-esc="o.picking_type_id.warehouse_id.partner_id.state_id.name" />
									<t t-esc="o.picking_type_id.warehouse_id.partner_id.country_id.name" />
								</span>
							</div>
						</div>
					</div>

					<!-- <div t-if="o.carrier_id"> <span style="font-size:14px;" t-field="o.carrier_id" 
						/> </div> -->
					<div t-if="o.note" style="font-size:14px;">
						Not:
						<span style="font-size:14px;" t-field="o.note" />
					</div>
					<table class="table table-condensed" style=";margin-bottom: 2px;">
						<thead>
							<tr style="font-size:10px;">
								<th>
									<strong>Sipariş (kaynak)</strong>
								</th>
								<th>
									<strong>Sorumlusu</strong>
								</th>
								<th>
									<strong>Tarih</strong>
								</th>
								<th name="td_sched_date_h">
									<strong>Planlanan Tarih</strong>
								</th>
							</tr>
						</thead>
						<tbody>
							<tr style="font-size:10px;">
								<td>
									<span t-field="o.origin" />
								</td>
								<td>
									<!-- <span t-field="o.x_hazirlayan" /> -->
								</td>
								<td>
									<span t-field="o.date" />
								</td>
								<td name="td_sched_date">
									<span t-field="o.min_date" />
								</td>
							</tr>
						</tbody>
					</table>


					<table class="table table-condensed" t-if="not o.pack_operation_ids">
						<thead>
							<tr>
								<th>
									<strong>Ürün</strong>
								</th>
								<th class="text-right">
									<strong>Miktar</strong>
								</th>
								<t t-if="o.picking_type_id.code != 'incoming'">
									<th>
										<strong>Kaynak</strong>
									</th>
								</t>
								<th>
									<strong>Barkod</strong>
								</th>
								<t t-if="o.picking_type_id.code != 'outgoing'">
									<th>
										<strong>Hedef</strong>
									</th>
								</t>
							</tr>
						</thead>
						<tbody>
							<tr t-foreach="o.move_lines" t-as="move">
								<t
									t-if="(o.state != 'partially_available') or (move.reserved_availability &gt; 0)">
									<td>
										<span style="font-size:12px;">
											[
											<t t-esc="move.product_id.default_code" />
											]
										</span>
										<span style="font-size:10px;">
											Yeri:[
											<t t-esc="move.product_id.loc_rack" />
											]
										</span>
										<br />
										<span style="font-size:10px;">
											<t t-esc="move.product_id.name" />
										</span>
									</td>
									<td class="text-right" style="font-size:12px;">
										<t t-if="o.state == 'partially_available'">
											<span t-field="move.reserved_availability" />
											<span t-field="move.product_id.uom_id" groups="product.group_uom" />
										</t>
										<t t-if="o.state != 'partially_available'">
											<t t-if="move.product_uom.id in [1,19,29,30]">
												<t t-esc="'%.0f'%move.product_uom_qty" />
												<span t-field="move.product_uom" groups="product.group_uom" />
											</t>
											<t t-if="not(move.product_uom.id in [1,19,29,30])">
												<span t-field="move.product_uom_qty" />
												<span t-field="move.product_uom" groups="product.group_uom" />
											</t>

										</t>
									</td>
									<t t-if="o.picking_type_id.code != 'incoming'">
										<td>
											<span style="font-size:8px;">
												<span t-field="move.location_id" />
												<br />
												Stokta:
												<span t-field="move.product_id.qty_available" />
												<span t-field="move.product_id.uom_id.name" />
												<br />
												<span t-field="move.string_availability_info" />
											</span>
										</td>
									</t>
									<td>
										<span t-if="move.product_id and move.product_id.ean13">
											<img
												t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('EAN13', move.product_id.ean13, 500, 80)"
												style="width:240px;height:40px" />
										</span>
									</td>
									<t t-if="o.picking_type_id.code != 'outgoing'">
										<td>
											<span t-field="move.location_dest_id" />
										</td>
									</t>
									<t t-if="move.product_id.pop_bom_in_reports">
										<t t-if="len(move.product_id.bom_ids) >= 1 ">
											<t t-foreach="move.product_id.bom_ids" t-as="line">
												<tr t-foreach="line.bom_line_ids" t-as="l">
													<td style="padding-left:30px;">
														<span t-field="line.routing_id.name" />
													</td>
													<td style="text-align:right;">
														<t t-if="l.product_id.default_code">
															[
															<span t-field="l.product_id.default_code" />
															]
														</t>
													</td>
													<td>
														<span t-field="l.product_id.name" />
													</td>
													<td>
														Location:
														<span t-field="move.location_id.name" />
													</td>
													<td>
														<span t-field="l.product_qty" />
													</td>
													<td>
														<span t-field="l.product_uom.name" />
													</td>
												</tr>
											</t>
										</t>
									</t>
								</t>
							</tr>
						</tbody>
					</table>

					<table class="table table-condensed" t-if="o.pack_operation_ids">
						<thead>
							<tr>
								<th>
									<strong>Ürün</strong>
								</th>
								<th class="text-right">
									<strong>Miktar</strong>
								</th>
								<t t-if="o.picking_type_id.code != 'incoming'">
									<th>
										<strong>Kaynak</strong>
									</th>
								</t>
								<th>
									<strong>Barcode</strong>
								</th>
								<t t-if="o.picking_type_id.code != 'outgoing'">
									<th>
										<strong>Hedef</strong>
									</th>
								</t>
							</tr>
						</thead>
						<tbody>
							<t t-foreach="o.pack_operation_ids" t-as="pack_operation">
								<tr>
									<td>
										<span style="font-size:8px;" t-field="pack_operation.product_id" />

										<t
											t-if="not pack_operation.product_id and pack_operation.package_id">
											<span t-field="pack_operation.package_id" />
										</t>
									</td>
									<td class="text-right" style="font-size:12px;">
										<t t-if="pack_operation.product_uom_id.id in [1,19,29,30]">
											<t t-esc="'%.0f'%pack_operation.product_qty" />
											<span t-field="pack_operation.product_uom_id" groups="product.group_uom" />
										</t>
										<t t-if="not(pack_operation.product_uom_id.id in [1,19,29,30])">
											<t t-esc="pack_operation.product_qty" />
											<span t-field="pack_operation.product_uom_id" groups="product.group_uom" />
										</t>
									</td>
									<t t-if="o.picking_type_id.code != 'incoming'">
										<td>
											<span style="font-size:8px;">
												<span t-field="move.location_id" />
												<br />
												Stokta:
												<span t-field="move.product_id.qty_available" />
												<span t-field="move.product_id.uom_id.name" />
												<br />
												<span t-field="move.string_availability_info" />
											</span>
											<span t-if="pack_operation.package_id">:</span>
											<span t-field="pack_operation.package_id" />
											<span t-if="pack_operation.lot_id">:</span>
											<span t-field="pack_operation.lot_id" />
										</td>
									</t>
									<td>
										<span t-if="pack_operation.lot_id">
											<img
												t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', pack_operation.lot_id.name, 500, 80)"
												style="width:240px;height:40px" />
										</span>
										<span
											t-if="pack_operation.product_id and not pack_operation.lot_id and pack_operation.product_id.ean13">
											<img
												t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('EAN13', pack_operation.product_id.ean13, 500, 80)"
												style="width:240px;height:40px" />
										</span>
										<span
											t-if="pack_operation.package_id and not pack_operation.product_id">
											<img
												t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', pack_operation.package_id.name, 500, 80)"
												style="width:240px;height:40px" />
										</span>
									</td>
									<t t-if="o.picking_type_id.code != 'outgoing'">
										<td>
											<span t-field="pack_operation.location_dest_id" />
											<span t-if="pack_operation.result_package_id">:</span>
											<span t-field="pack_operation.result_package_id" />
										</td>
									</t>

								</tr>

								<t t-if="pack_operation.product_id.pop_bom_in_reports">
									<t t-if="len(pack_operation.product_id.bom_ids) &gt;= 1 ">
										<t t-foreach="pack_operation.product_id.bom_ids" t-as="l">
											<tr>
												<table>
													<tr>
														<t t-if="l.routing_id.name">
															<td style="padding-left:30px;">
																<span t-field="l.routing_id.name" />
															</td>
														</t>
														<td>
															<t t-foreach="l.bom_line_ids" t-as="bom_line">
																<span t-field="l.product_id.default_code" />
																<span t-field="l.product_id.name" />
																<span t-field="l.product_id.loc_rack" />
																<span t-field="l.product_qty" />
																<span t-field="l.product_uom" />

															</t>
														</td>
													</tr>
												</table>
											</tr>
										</t>
									</t>
								</t>
							</t>

						</tbody>
					</table>


				</div>



				<div class="footer" style="font-size:10px;">
					<div class="text-center" style="border-top: 1px solid black;">
						<ul class="list-inline">
							<li t-if="o.name">
								<span t-field="o.name" />
							</li>
							<li t-if="o.name">&amp;bull;</li>
							<li>
								Yazan:
								<span t-esc="user.name" />
							</li>
							<li>&amp;bull;</li>
							<li>
								Baskı Tarih Saat:
								<span
									t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y-%m-%d %H:%M:%S')" />
							</li>
						</ul>
						<ul class="list-inline">
							<li>Sayfa:</li>
							<li>
								<span class="page" />
							</li>
							<li>/</li>
							<li>
								<span class="topage" />
							</li>
						</ul>
					</div>
				</div>
			</t>
		</template>

		<template id="report_stock_picking_altinkaya">
			<t t-call="report.html_container">
				<t t-foreach="doc_ids" t-as="doc_id">
					<t
						t-raw="translate_doc(doc_id, doc_model, 'partner_id.lang', 'altinkaya_warehouse.report_stock_picking_altinkaya_doc')" />
				</t>
			</t>
		</template>

	</data>
</openerp>
